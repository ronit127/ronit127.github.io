{
  "name": "portfolio-website",
  "version": "1.0.0",
  "description": "A simple and clean portfolio website with a grey color scheme.",
  "main": "src/index.jsx",
  "scripts": {
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && concurrently \"npm:watch:css\" \"react-scripts start\"",
    "build": "set NODE_OPTIONS=--openssl-legacy-provider && npm run build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch:css": "tailwindcss -i ./src/styles/tailwind.css -o ./src/index.css --watch",
    "build:css": "tailwindcss -i ./src/styles/tailwind.css -o ./src/index.css --minify",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "keywords": [
    "portfolio",
    "react",
    "website",
    "whimsical",
    "pastel"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "gsap": "^3.13.0",
    "lucide-react": "^0.546.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.30.2",
    "react-scripts": "^5.0.1",
    "three": "^0.181.2"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7"
  },
  "homepage": "https://ronit127.github.io",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
